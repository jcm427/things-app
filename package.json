{
  "name": "things-app",
  "description": "The gateway to your Library of Things.",
  "version": "1.0.0-alpha",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npm-run-all --parallel dev:app dev:server",
    "dev:app": "sapper dev",
    "dev:server": "netlify-lambda serve functions",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "netlify:build": "run-p netlify:build:**",
    "netlify:build:app": "sapper export",
    "netlify:build:lambda": "netlify-lambda build functions"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "express": "^4.17.1",
    "libphonenumber-js": "^1.9.6",
    "netlify-lambda": "^2.0.1",
    "node-fetch": "^2.6.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.8.6",
    "http-proxy-middleware": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-nesting": "^7.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.5.2",
    "svelte-typewriter": "^2.3.1",
    "tailwindcss": "^2.0.1"
